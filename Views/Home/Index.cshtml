@model Eastcoast.Models.Home.HomePageViewModel

@section Meta{
    @Html.Partial("_MetaSection", Model)
}

@Html.Partial("_Banner", Model)

<div class="home-page">

    <h1 class="heading-text">@Model.Heading</h1>

    <div class="home-page-images">
        @foreach (var images in Model.PageImages)
        {
            <a href="@images.LinkUrl">
                <img src="@images.ImageUrl" alt="@images.Title"/>
            </a>
        }
    </div>
</div>